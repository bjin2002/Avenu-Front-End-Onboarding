.setItem(LOCAL_STORAGE_KEY, JSON.stringify(todos))\n  }, [todos])\n\n  function toggleTodo(id) {\n    const newTodos = [...todos]\n    const todos = newTodos.find(todo => todo.id=== id)\n    todo.complete != todo.complete\n    setTodos(newTodos)\n  }\n \n  function handleAddTodo(e) {\n    const name = todoNameRef.current.value\n\n    if(name === '') return\n    setTodos(prevTodos=> {\n      return [...prevTodos, {id: uuidv4(), name: name, complete: false}]\n    })\n    todoNameRef.current.value = null\n  }\n\n  function handleClearTodo(e) {\n    const newTodos = todos.filter(tood => !todo.complete)\n    setTodos(newTodos)\n  }\n\n  return (\n    <>\n      <TodoList todos={todos} toggleTodo = {toggleTodo}/>\n      <input ref = {todoNameRef} type = \"text\"/>\n      <button onClick = {handleAddTodo}>  Add Todo</button>\n      <button onClick = {handleClearTodo}>  Clear Complete </button>\n      <div>{todos.filter(todo => !todo.complete).length} left to do</div>\n    </>\n  )\n}\n\nexport default App;\n","/Users/Ben/Downloads/Avenu/front-end-onboarding/src/reportWebVitals.js",[],"/Users/Ben/Downloads/Avenu/front-end-onboarding/src/TodoList.js",[],"/Users/Ben/Downloads/Avenu/front-end-onboarding/src/Todo.js",[],{"ruleId":"30","severity":1,"message":"31","line":22,"column":26,"nodeType":"32","messageId":"33","endLine":22,"endColumn":31},{"ruleId":"34","severity":2,"message":"35","line":24,"column":5,"nodeType":"36","messageId":"37","endLine":24,"endColumn":35},{"ruleId":"38","severity":1,"message":"39","line":24,"column":5,"nodeType":"40","messageId":"41","endLine":24,"endColumn":35},{"ruleId":"42","severity":2,"message":"43","line":24,"column":5,"nodeType":"32","messageId":"44","endLine":24,"endColumn":9},{"ruleId":"45","severity":1,"message":"46","line":24,"column":19,"nodeType":"40","messageId":"47","endLine":24,"endColumn":21},{"ruleId":"42","severity":2,"message":"43","line":24,"column":22,"nodeType":"32","messageId":"44","endLine":24,"endColumn":26},{"ruleId":"42",